<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <AssemblyName>Orleans.EventSourcing.Snapshot</AssemblyName>
    <RootNamespace>Orleans.EventSourcing.Snapshot</RootNamespace>
    <Version>1.0.0</Version>
    <Product />
    <Description>Snapshot storage provider for orleans event sourcing</Description>
    <PackageProjectUrl>https://github.com/AElfProject/Orleans.EventSourcing.Snapshot</PackageProjectUrl>
    <PackageId>Orleans.EventSourcing.Snapshot</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DeepCloner" Version="0.10.2" />
    <PackageReference Include="Microsoft.Orleans.EventSourcing" Version="3.6.5" />
  </ItemGroup>

</Project>
